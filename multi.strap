[General]
arch		= amd64
directory	= ./andbotfs/
cleanup		= true
noauth		= true
unpack		= true
ignorenativearch	= true
debootstrap	= Debian
aptsources	= Debian
setupscript	= ./setup.sh
configscript	= ./config.sh

[Debian]
keyring		= debian-archive-keyring
packages	= locales adduser sudo apt git-core gnupg flex bison gperf build-essential zip unzip curl openjdk-6-jdk libxml2-utils xsltproc schedtool file pngcrush ia32-libs nano python
source		= http://ftp.debian.org/debian
suite		= wheezy
